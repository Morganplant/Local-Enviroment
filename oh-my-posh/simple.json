        //     "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
        //     "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
        //     "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
        //   ],
        //   "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }} ",
        //   "properties": {
        //     "discharging_icon": "󱊡 ",
        //     "charging_icon": "󰂄 ",
        //     "charged_icon": "󱊣 "
        //   }
        // }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#242424",
          "foreground": "#33DD2D",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f77622{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e43b44{{ end }}",
            "{{ if gt .Ahead 0 }}#2ce8f5{{ end }}",
            "{{ if gt .Behind 0 }}#ff7688{{ end }}"
          ],
          "powerline_symbol": "\ue0b0",
          "properties": {
            // "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Working.Changed }}<cyan> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<yellow> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} |<green> \udb80\udd93 {{ .StashCount }}</>{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "background": "#242424",
          "foreground": "#1BD760",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "powerline_symbol": "\ue0b0",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .Env.SPOTIFY }}\uf1bc {{ .Env.SPOTIFY }}‎ {{ end }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#242424",
          "foreground": "#007acc",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "#242424",
          "foreground": "#f1184c",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "powerline_symbol": "\ue0b0",
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#242424",
          "foreground": "#FFBB00",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        },
        {
          "background": "#242424",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0044{{ end }}"
          ],
          "foreground": "#0080ff",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \uf0e7 ",
          "trailing_diamond": "\ue0b0",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
