{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#242424",
          "foreground": "#008000",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "template": "{{ if .Env.VIRTUAL_ENV_MTP_PROMPT }} \ue73c {{ .Env.VIRTUAL_ENV_MTP_PROMPT }} {{ end }}",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
